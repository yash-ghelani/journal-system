            <PasswordField fx:id="passWordField" layoutX="113.0" layoutY="445.0" prefHeight="35.0" prefWidth="205.0" promptText="Password" />
            <ChoiceBox fx:id="roles" id="choiceBox" onAction="#handleRoles" layoutX="113.0" layoutY="176.0" prefHeight="28.0" prefWidth="158.0" />
         </children>
      </AnchorPane>
   </children>
</VBox>
